  "copyFormatting": "none",
  "copyOnSelect": false,
  "alwaysShowTabs": true,
  "initialCols": 120,
  "initialRows": 30,
  "requestedTheme": "system",
  "showTabsInTitlebar": true,
  "showTerminalTitleInTitlebar": true,
  "confirmCloseAllTabs": false,
  "wordDelimiters": " ./\\()\"'-:,.;<>~!@#$%^&*|+=[]{}~?\u2502",
  "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
  "actions": [
    {
      "command": "copy",
      "keys": ["ctrl+shift+c"]
    },
    {
      "command": "paste",
      "keys": ["ctrl+shift+v"]
    },
    {
      "command": "find",
      "keys": "ctrl+shift+f"
    },
    {
      "command": "closePane",
      "keys": ["ctrl+w"]
    },
    {
      "command": "newTab",
      "keys": ["ctrl+t"]
    },
    {
      "command": "nextTab",
      "keys": ["ctrl+tab"]
    },
    {
      "command": "prevTab",
      "keys": ["ctrl+shift+tab"]
    },
    {
      "command": "openSettings",
      "keys": ["ctrl+,"]
    },
    {
      "command": "duplicateTab",
      "keys": ["ctrl+shift+d"]
    },
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "keys": "alt+shift+d"
    }
  ],
